{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["standard", "plugin:react/recommended", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": { "react/react-in-jsx-scope": "off" }
}
